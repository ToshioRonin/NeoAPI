generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id         Int      @id @default(autoincrement())
  First_Name String
  Last_Name  String
  UserName   String
  email      String   @unique
  phone      String
  password   String
  Genero     String
  role       Role     @default(USER)
}

enum Role{
  USER
  ADMIN
}

enum Sexo{
  MASCULINO
  FEMENINO
  OTRO
}